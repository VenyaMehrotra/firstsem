//Write a program to check if the diagonals of a matrix are distinct
#include<stdio.h>
int main()
{
    int i,j,rows,columns,A[10][10];
    printf("Enter the number of rows and columns=");
    scanf("%d %d",&rows,&columns);
    int diag[rows];
    if(rows!=columns)
    {
        printf("Please enter a square matrix");
        return 0;
    }
    printf("Enter the elements=");
     for(i = 0;i <rows;i++)
    {
        for(j=0;j<columns;j++)
        {
            scanf("%d",&A[i][j]);
        }
    }
    for(i=0;i<rows;i++)
    {
        diag[i]=A[i][i];
    }
    for(i=0;i<rows;i++)
    {
        for(j=i+1;j<columns;j++)
        {
        if(diag[i]!=diag[j])
        {
            printf("True");
            return 0;
        }
        }
    }
    printf("False");
    return 0;
}
