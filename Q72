/*Q72 (2D Arrays)
Find the sum of all elements in a matrix.*/
#include<stdio.h>
int main()
{
    int A[50][50];
    int m, n, i, j,sum=0;
    printf("Enter the number of rows and columns of matrix = ");
    scanf("%d %d",&m,&n);
    printf("Enter elements of Matrix A (%d x %d) in row-major order: \n",m,n);
    for(i = 0; i < m; i++)
    {
        for( j = 0 ; j < n; j++)
        {
            scanf("%d",&A[i][j]);
            sum=sum+A[i][j];
        }
    }
    printf("Sum= %d",sum);
    return 0;
}
